<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <PackageId>RvdB.Common.Azure</PackageId>
    <Version>3.0.0</Version>
    <Authors>Rick van den Bosch</Authors>
    <Company>Rick van den Bosch</Company>
    <Copyright>Copyright Â© 2020 - 2022 Rick van den Bosch</Copyright>
    <Description>Package with (parts of) my own Azure toolbox that I use in multiple projects.</Description>
    <PackageReleaseNotes>Huge refactor (now usable with DI). Added readme, included examples.</PackageReleaseNotes>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/rickvdbosch/rvdb-common</PackageProjectUrl>
    <RepositoryUrl>https://github.com/rickvdbosch/rvdb-common</RepositoryUrl>
    <PackageTags>RvdB;rickvdbosch;azure;helper;storage;blobstorage;tablestorage</PackageTags>
